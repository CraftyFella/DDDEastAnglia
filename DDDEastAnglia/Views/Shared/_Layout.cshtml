<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>DDD East Anglia @ViewBag.Title</title>
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="description" content="DDD East Anglia conference website 29 June 2013">
    <meta name="author" content="DDD East Anglia">
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/Markdown")
    @Scripts.Render("~/bundles/modernizr")

    <!-- HTML5 shim, for IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9]>
      <script src="~/Scripts/html5shiv.js"></script>
      <script src="~/Scripts/html5shiv-printshiv.js"></script>
    <![endif]-->
    <!-- Fav and touch icons -->
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="../assets/ico/apple-touch-icon-144-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="114x114" href="../assets/ico/apple-touch-icon-114-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="72x72" href="../assets/ico/apple-touch-icon-72-precomposed.png">
    <link rel="apple-touch-icon-precomposed" href="../assets/ico/apple-touch-icon-57-precomposed.png">
    <link rel="shortcut icon" href="~/favicon.ico">
</head>
    <body>
        <div class="navbar navbar-inverse navbar-fixed-top">
            <div class="navbar-inner">
                <div class="container-fluid">
                    <button type="button" class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a class="brand" href="@Url.Action(MVC.Home.Index())">Project name</a>
                    <div class="nav-collapse collapse">
                        <p class="btn-group pull-right">
                            @Html.Partial("_LoginPartial")
                        </p>
                        <ul class="nav">
                            <li class="active">@Html.ActionLink("Home", MVC.Home.Index())</li>
                            <li>@Html.ActionLink("Venue", MVC.Home.Venue())</li>
                            <li>@Html.ActionLink("Sponsors", MVC.Home.Sponsors())</li>
                            <li>@Html.ActionLink("Team", MVC.Home.Team())</li>
                            <li>@Html.ActionLink("Sessions", MVC.Session.Index())</li>
                            <li>@Html.ActionLink("Contact", "Contact", "Home")</li>
                        </ul>
                    </div><!--/.nav-collapse -->
                </div>
            </div>
        </div>
        <div class="container-fluid">
            <div class="row-fluid">
                <div class="span3">
                    <div class="well sidebar-nav">
                        <ul class="nav nav-list">
                            <li class="nav-header">Sidebar</li>
                            <li class="active"><a href="#">Link</a></li>
                            <li><a href="#">Link</a></li>
                            <li><a href="#">Link</a></li>
                            <li><a href="#">Link</a></li>
                            <li class="nav-header">Sidebar</li>
                            <li><a href="#">Link</a></li>
                            <li><a href="#">Link</a></li>
                            <li><a href="#">Link</a></li>
                            <li><a href="#">Link</a></li>
                            <li><a href="#">Link</a></li>
                            <li><a href="#">Link</a></li>
                            <li class="nav-header">Sidebar</li>
                            <li><a href="#">Link</a></li>
                            <li><a href="#">Link</a></li>
                            <li><a href="#">Link</a></li>
                        </ul>
                    </div><!--/.well -->
                </div><!--/span-->
          
                <div class="span9">
                    @RenderSection("featured", required: false)
                    <section class="content-wrapper main-content clear-fix">
                        @RenderBody()
                    </section>
                </div>
            </div>
            <hr>

            <footer>
            <p>&copy; @DateTime.Now.Year - DDD Community</p>
            </footer>
        </div><!--/.fluid-container-->

        @Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")
        @Styles.Render("~/Content/Markdown")
        @RenderSection("scripts", required: false)

        @{Html.RenderPartial(MVC.Shared.Views._GoogleAnalytics);}
    </body>
</html>
