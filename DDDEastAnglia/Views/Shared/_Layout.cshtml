<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>DDD East Anglia @ViewBag.Title</title>
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="description" content="DDD East Anglia conference website 29 June 2013">
    <meta name="author" content="DDD East Anglia">
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/Markdown")
    @Scripts.Render("~/bundles/modernizr")

    <!-- HTML5 shim, for IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9]>
      <script src="~/Scripts/html5shiv.js"></script>
      <script src="~/Scripts/html5shiv-printshiv.js"></script>
    <![endif]-->
    <!-- Fav and touch icons -->
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="../assets/ico/apple-touch-icon-144-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="114x114" href="../assets/ico/apple-touch-icon-114-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="72x72" href="../assets/ico/apple-touch-icon-72-precomposed.png">
    <link rel="apple-touch-icon-precomposed" href="../assets/ico/apple-touch-icon-57-precomposed.png">
    <link rel="shortcut icon" href="~/favicon.ico">
</head>
    <body>
        <div class="container-fluid">
            <div class="masthead">
                <img src="~/Images/logo-small.png" class="pull-left" />
                <h3 class="muted">DDD East Anglia</h3>
                <p>Cambridge, UK<br />
                Saturday 29 June 2013</p>
            </div>
        </div>
        <div class="navbar navbar-static-top">
            <div class="navbar-inner">
                <div class="container-fluid">
                    <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </a>
                    <div class="btn-group pull-right">
                        @Html.Partial("_LoginPartial")
                    </div>
                    <div class="nav-collapse">
                        <ul class="nav">
                            <li class="dropdown active">@Html.ActionLink("Home", MVC.Home.Index())</li>
                            <li class="dropdown">@Html.ActionLink("Venue", MVC.Home.Venue())</li>
                            <li class="dropdown">@Html.ActionLink("Sponsors", MVC.Home.Sponsors())</li>
                            <li class="dropdown">@Html.ActionLink("Team", MVC.Home.Team())</li>
                            <li class="dropdown">@Html.ActionLink("Sessions", MVC.Session.Index())</li>
                            <li class="dropdown">@Html.ActionLink("Contact", "Contact", "Home")</li>
                        </ul>
                    </div><!--/.nav-collapse -->
                </div>
            </div>
        </div>
        <div class="container-fluid">
            <div class="container-fluid">
                <div class="row-fluid">
                    @*<div class="span3">
                    <div class="well">
                        
                    </div><!--/.well -->
                </div><!--/span3-->*@
                    <div class="span9">
                        @RenderSection("featured", required: false)
                        <div class="row-fluid">
                            @RenderBody()
                        </div>
                    </div><!--/span9-->
                    <div class="span3" id="sponsors">
                        <div class="sponsor">
                            <!-- Sponsors go here -->
                        </div>
                    </div>
                </div>
                <hr>
                <footer>
                    <p>&copy; @DateTime.Now.Year - DDD Community</p>
                </footer>
            </div>
        </div><!--/.fluid-container-->

        @Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")
        @Styles.Render("~/Content/Markdown")
        @RenderSection("scripts", required: false)

        @{Html.RenderPartial(MVC.Shared.Views._GoogleAnalytics);}
    </body>
</html>
