@model DDDEastAnglia.Areas.Admin.Models.VotersPerIPAddressViewModel

<h2>Voters per IP Address</h2>

<table class="table table-striped table-hover table-condensed">
    <thead>
        <tr><th>
            IP Address
        </th><th>
            Number of Voters
        </th></tr>
    </thead>

    <tbody>
        @foreach (var item in @Model.IPAddressVoters)
        {
            <tr><td>
                @item.IPAddress
            </td><td>
                @item.NumberOfVoters
            </td></tr>
        }
    </tbody>
</table>
