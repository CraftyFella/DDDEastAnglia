@model DDDEastAnglia.Models.Sponsor

@{
    ViewBag.Title = string.Format("Delete {0}?", Model.Name);
}

<h2>@ViewBag.Title</h2>

<p>Deleting a sponsor cannot be undone. Are you <em>really</em> sure you want to delete this sponsor?</p>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    <p>
        <input type="submit" class="btn btn-danger" value="Delete sponsor" />
        @Html.ActionLink("Cancel", "Index")
    </p>
}
